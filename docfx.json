{
  "metadata": [
    {
      "src": [
        {
          "files": ["bin/Release/net8.0/Integrity2D.dll"]
        }
      ],
      "dest": "api",
      "properties": {
        "Configuration": "Release",
        "DocumentAssembliesWith": "System.Object"
      },
      "filter": "filter.yml"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "index.md",
          "toc.yml"
        ],
        "exclude": [
          "README.md",
          "obj/**",
          "docs/**"
        ]
      },
      {
        "files": [
          "api/toc.yml",
          "api/**/*.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": ["DefaultEngineAssets/**"]
      }
    ],
    "globalMetadata": {
      "_disableGitFeatures": true
    },
    "overwrite": [],
    "dest": "docs",
    "templates": ["default"],
    "markdownEngineName": "markdig",
    "baseurl": "/Integrity2D/"
  }
}
